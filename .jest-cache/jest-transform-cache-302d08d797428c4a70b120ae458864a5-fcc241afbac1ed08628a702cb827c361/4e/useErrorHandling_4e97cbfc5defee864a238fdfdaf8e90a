  /* istanbul ignore next */
  cov_2i91n94o49().s[14]++;
  const getErrorMessage = (error, type) => {
    /* istanbul ignore next */
    cov_2i91n94o49().f[4]++;
    cov_2i91n94o49().s[15]++;
    switch (type) {
