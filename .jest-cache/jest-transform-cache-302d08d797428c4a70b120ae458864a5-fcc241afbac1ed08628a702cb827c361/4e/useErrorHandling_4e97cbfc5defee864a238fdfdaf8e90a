        /* istanbul ignore next */
        cov_2i91n94o49().b[3][1]++;
        cov_2i91n94o49().s[17]++;
        return "Network connection issue. Please check your internet connection.";
      case ERROR_TYPES.GAME_ERROR:
        /* istanbul ignore next */
        cov_2i91n94o49().b[3][2]++;
        cov_2i91n94o49().s[18]++;
        return "Game encountered an error. Please try again.";
      default:
        /* istanbul ignore next */
        cov_2i91n94o49().b[3][3]++;
        cov_2i91n94o49().s[19]++;
        return /* istanbul ignore next */(cov_2i91n94o49().b[4][0]++, error?.message) ||
        /* istanbul ignore next */
        (cov_2i91n94o49().b[4][1]++, "Something went wrong. Please try again.");
    }
  };

  // Clear error
  const clearError =
  /* istanbul ignore next */
  (cov_2i91n94o49().s[20]++, useCallback(() => {
    /* istanbul ignore next */
    cov_2i91n94o49().f[5]++;
    cov_2i91n94o49().s[21]++;
    setError(null);
    /* istanbul ignore next */
    cov_2i91n94o49().s[22]++;
    setRetryCount(0);
  }, []));

  // Retry function
  const retry =
  /* istanbul ignore next */
  (cov_2i91n94o49().s[23]++, useCallback(retryFn => {
    /* istanbul ignore next */
    cov_2i91n94o49().f[6]++;
    cov_2i91n94o49().s[24]++;
    if (
    /* istanbul ignore next */
    (cov_2i91n94o49().b[6][0]++, retryFn) &&
    /* istanbul ignore next */
    (cov_2i91n94o49().b[6][1]++, typeof retryFn === 'function')) {
      /* istanbul ignore next */
      cov_2i91n94o49().b[5][0]++;
      cov_2i91n94o49().s[25]++;
      setIsLoading(true);
      /* istanbul ignore next */
      cov_2i91n94o49().s[26]++;
      setError(null);
      /* istanbul ignore next */
      cov_2i91n94o49().s[27]++;
      try {
        /* istanbul ignore next */
        cov_2i91n94o49().s[28]++;
        retryFn();
      } catch (err) {
        /* istanbul ignore next */
        cov_2i91n94o49().s[29]++;
        handleError(err, ERROR_TYPES.UNKNOWN_ERROR);
      } finally {
        /* istanbul ignore next */
        cov_2i91n94o49().s[30]++;
        setIsLoading(false);
      }
    } else
    /* istanbul ignore next */
    {
      cov_2i91n94o49().b[5][1]++;
    }
  }, [handleError]));

  // Loading state management
  const setLoading =
  /* istanbul ignore next */
  (cov_2i91n94o49().s[31]++, useCallback(loading => {
    /* istanbul ignore next */
    cov_2i91n94o49().f[7]++;
    cov_2i91n94o49().s[32]++;
    setIsLoading(loading);
  }, []));

  // Error boundary handler
  const handleErrorBoundary =
  /* istanbul ignore next */
  (cov_2i91n94o49().s[33]++, useCallback((error, errorInfo) => {
    /* istanbul ignore next */
    cov_2i91n94o49().f[8]++;
    cov_2i91n94o49().s[34]++;
    console.error('Error Boundary caught an error:', error, errorInfo);
    const errorData =
    /* istanbul ignore next */
    (cov_2i91n94o49().s[35]++, {
      type: ERROR_TYPES.UNKNOWN_ERROR,
      message: "The app encountered an unexpected error. Please refresh the page.",
      timestamp: new Date().toISOString(),
      componentStack: errorInfo.componentStack,
      retryCount: 0
    });
    /* istanbul ignore next */
    cov_2i91n94o49().s[36]++;
    setError(errorData);
  }, []));

  // Auto-clear errors after timeout
  /* istanbul ignore next */
  cov_2i91n94o49().s[37]++;
  useEffect(() => {
    /* istanbul ignore next */
    cov_2i91n94o49().f[9]++;
    cov_2i91n94o49().s[38]++;
    if (
    /* istanbul ignore next */
    (cov_2i91n94o49().b[8][0]++, error) &&
    /* istanbul ignore next */
    (cov_2i91n94o49().b[8][1]++, error.type !== ERROR_TYPES.AUDIO_ERROR)) {
      /* istanbul ignore next */
      cov_2i91n94o49().b[7][0]++;
      const timer =
      /* istanbul ignore next */
      (cov_2i91n94o49().s[39]++, setTimeout(() => {
        /* istanbul ignore next */
        cov_2i91n94o49().f[10]++;
        cov_2i91n94o49().s[40]++;
        clearError();
      }, 10000)); // Auto-clear after 10 seconds
      /* istanbul ignore next */
      cov_2i91n94o49().s[41]++;
      return () => {
        /* istanbul ignore next */
        cov_2i91n94o49().f[11]++;
        cov_2i91n94o49().s[42]++;
        return clearTimeout(timer);
      };
    } else
    /* istanbul ignore next */
    {
      cov_2i91n94o49().b[7][1]++;
    }
  }, [error, clearError]);
  /* istanbul ignore next */
  cov_2i91n94o49().s[43]++;
  return {
    error,
    isLoading,
    retryCount,
    ERROR_TYPES,
    handleError,
    clearError,
    retry,
    setLoading,
    handleErrorBoundary
  };
};
