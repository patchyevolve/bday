  cov_1saqpwvg6u().f[0]++;
  const gameLoopRef =
  /* istanbul ignore next */
  (cov_1saqpwvg6u().s[1]++, useRef(null));
  const lastTileSpawnRef =
  /* istanbul ignore next */
  (cov_1saqpwvg6u().s[2]++, useRef(0));
  const animationFrameRef =
  /* istanbul ignore next */
  (cov_1saqpwvg6u().s[3]++, useRef(null));
  const gameLoop =
  /* istanbul ignore next */
  (cov_1saqpwvg6u().s[4]++, useCallback(() => {
    /* istanbul ignore next */
    cov_1saqpwvg6u().f[1]++;
    cov_1saqpwvg6u().s[5]++;
    if (
    /* istanbul ignore next */
    (cov_1saqpwvg6u().b[1][0]++, !gameStarted) ||
    /* istanbul ignore next */
    (cov_1saqpwvg6u().b[1][1]++, gameOver) ||
    /* istanbul ignore next */
    (cov_1saqpwvg6u().b[1][2]++, gameCompleted)) {
      /* istanbul ignore next */
      cov_1saqpwvg6u().b[0][0]++;
      cov_1saqpwvg6u().s[6]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1saqpwvg6u().b[0][1]++;
    }
    const currentTime =
    /* istanbul ignore next */
    (cov_1saqpwvg6u().s[7]++, Date.now());

    // Spawn new tiles with dynamic timing
    const baseInterval =
    /* istanbul ignore next */
    (cov_1saqpwvg6u().s[8]++, activePowerUps.slowMotion.active ?
    /* istanbul ignore next */
    (cov_1saqpwvg6u().b[2][0]++, 2500) :
    /* istanbul ignore next */
    (cov_1saqpwvg6u().b[2][1]++, GAME_CONFIG.BASE_SPAWN_INTERVAL));
    const spawnInterval =
    /* istanbul ignore next */
    (cov_1saqpwvg6u().s[9]++, Math.max(baseInterval - tilesSpawned * 100, GAME_CONFIG.MIN_SPAWN_INTERVAL));

    // In endless mode, keep spawning tiles indefinitely
    const shouldSpawn =
    /* istanbul ignore next */
    (cov_1saqpwvg6u().s[10]++, endlessMode ?
    /* istanbul ignore next */
    (cov_1saqpwvg6u().b[3][0]++, true) :
    /* istanbul ignore next */
    (cov_1saqpwvg6u().b[3][1]++, tilesSpawned < happyBirthdayNotes.length));
    /* istanbul ignore next */
    cov_1saqpwvg6u().s[11]++;
    if (
    /* istanbul ignore next */
    (cov_1saqpwvg6u().b[5][0]++, currentTime - lastTileSpawnRef.current > spawnInterval) &&
    /* istanbul ignore next */
    (cov_1saqpwvg6u().b[5][1]++, shouldSpawn)) {
      /* istanbul ignore next */
      cov_1saqpwvg6u().b[4][0]++;
      const noteIndex =
      /* istanbul ignore next */
      (cov_1saqpwvg6u().s[12]++, endlessMode ?
      /* istanbul ignore next */
      (cov_1saqpwvg6u().b[6][0]++, tilesSpawned % happyBirthdayNotes.length) :
      /* istanbul ignore next */
      (cov_1saqpwvg6u().b[6][1]++, tilesSpawned));
      const newTile =
      /* istanbul ignore next */
      (cov_1saqpwvg6u().s[13]++, {
        id: Date.now() + Math.random(),
        note: happyBirthdayNotes[noteIndex],
        column: Math.floor(Math.random() * 4),
        y: -80,
        speed: activePowerUps.slowMotion.active ?
        /* istanbul ignore next */
        (cov_1saqpwvg6u().b[7][0]++, 2 + Math.floor(tilesSpawned / 5)) :
        /* istanbul ignore next */
        (cov_1saqpwvg6u().b[7][1]++, GAME_CONFIG.BASE_TILE_SPEED + Math.floor(tilesSpawned / 3)),
        noteIndex: noteIndex,
        spawned: true
      });
      /* istanbul ignore next */
      cov_1saqpwvg6u().s[14]++;
      onSpawnTile(newTile);
      /* istanbul ignore next */
      cov_1saqpwvg6u().s[15]++;
      lastTileSpawnRef.current = currentTime;

      // Chance to spawn power-up
      /* istanbul ignore next */
      cov_1saqpwvg6u().s[16]++;
      onSpawnPowerUp();
    } else
    /* istanbul ignore next */
    {
      cov_1saqpwvg6u().b[4][1]++;
    }

    // Update tile positions
    cov_1saqpwvg6u().s[17]++;
    onUpdateTiles(prevTiles => {
      /* istanbul ignore next */
      cov_1saqpwvg6u().f[2]++;
      cov_1saqpwvg6u().s[18]++;
      return prevTiles.map(tile => {
        /* istanbul ignore next */
        cov_1saqpwvg6u().f[3]++;
        cov_1saqpwvg6u().s[19]++;
        return {
          ...tile,
          y: tile.y + tile.speed
        };
      }).filter(tile => {
        /* istanbul ignore next */
        cov_1saqpwvg6u().f[4]++;
        cov_1saqpwvg6u().s[20]++;
        // Remove tiles that hit bottom (lose life unless invincible)
        if (tile.y > GAME_CONFIG.DANGER_ZONE_HEIGHT) {
          /* istanbul ignore next */
          cov_1saqpwvg6u().b[8][0]++;
          cov_1saqpwvg6u().s[21]++;
          if (!activePowerUps.invincible.active) {
            /* istanbul ignore next */
            cov_1saqpwvg6u().b[9][0]++;
            cov_1saqpwvg6u().s[22]++;
            onLoseLife();
          } else
          /* istanbul ignore next */
          {
            cov_1saqpwvg6u().b[9][1]++;
          }
          cov_1saqpwvg6u().s[23]++;
          return false;
        } else
        /* istanbul ignore next */
        {
          cov_1saqpwvg6u().b[8][1]++;
        }
        cov_1saqpwvg6u().s[24]++;
        return true;
      });
    });

    // Update power-up positions
    /* istanbul ignore next */
    cov_1saqpwvg6u().s[25]++;
    onUpdatePowerUps();

    // Schedule next frame
    /* istanbul ignore next */
    cov_1saqpwvg6u().s[26]++;
    animationFrameRef.current = requestAnimationFrame(gameLoop);
  }, [gameStarted, gameOver, gameCompleted, tilesSpawned, endlessMode, activePowerUps, happyBirthdayNotes, onSpawnTile, onUpdateTiles, onUpdatePowerUps, onSpawnPowerUp, onLoseLife]));

  // Start game loop when game starts
  /* istanbul ignore next */
  cov_1saqpwvg6u().s[27]++;
  useEffect(() => {
    /* istanbul ignore next */
    cov_1saqpwvg6u().f[5]++;
    cov_1saqpwvg6u().s[28]++;
    if (
    /* istanbul ignore next */
    (cov_1saqpwvg6u().b[11][0]++, gameStarted) &&
    /* istanbul ignore next */
    (cov_1saqpwvg6u().b[11][1]++, !gameOver) &&
    /* istanbul ignore next */
    (cov_1saqpwvg6u().b[11][2]++, !gameCompleted)) {
      /* istanbul ignore next */
      cov_1saqpwvg6u().b[10][0]++;
      cov_1saqpwvg6u().s[29]++;
      // Use requestAnimationFrame for smooth 60fps performance
      animationFrameRef.current = requestAnimationFrame(gameLoop);
    } else
    /* istanbul ignore next */
    {
      cov_1saqpwvg6u().b[10][1]++;
    }
    cov_1saqpwvg6u().s[30]++;
    return () => {
      /* istanbul ignore next */
      cov_1saqpwvg6u().f[6]++;
      cov_1saqpwvg6u().s[31]++;
      if (animationFrameRef.current) {
        /* istanbul ignore next */
        cov_1saqpwvg6u().b[12][0]++;
        cov_1saqpwvg6u().s[32]++;
        cancelAnimationFrame(animationFrameRef.current);
      } else
      /* istanbul ignore next */
      {
        cov_1saqpwvg6u().b[12][1]++;
      }
    };
  }, [gameStarted, gameOver, gameCompleted, gameLoop]);

  // Handle game over when lives reach 0
  /* istanbul ignore next */
  cov_1saqpwvg6u().s[33]++;
  useEffect(() => {
    /* istanbul ignore next */
    cov_1saqpwvg6u().f[7]++;
    cov_1saqpwvg6u().s[34]++;
    if (gameOver) {
      /* istanbul ignore next */
      cov_1saqpwvg6u().b[13][0]++;
      cov_1saqpwvg6u().s[35]++;
      onGameOver();
    } else
    /* istanbul ignore next */
    {
      cov_1saqpwvg6u().b[13][1]++;
    }
  }, [gameOver, onGameOver]);
  /* istanbul ignore next */
  cov_1saqpwvg6u().s[36]++;
  return {
    startGameLoop: () => {
      /* istanbul ignore next */
      cov_1saqpwvg6u().f[8]++;
      cov_1saqpwvg6u().s[37]++;
      if (!animationFrameRef.current) {
        /* istanbul ignore next */
        cov_1saqpwvg6u().b[14][0]++;
        cov_1saqpwvg6u().s[38]++;
        animationFrameRef.current = requestAnimationFrame(gameLoop);
      } else
      /* istanbul ignore next */
      {
        cov_1saqpwvg6u().b[14][1]++;
      }
    },
    stopGameLoop: () => {
      /* istanbul ignore next */
      cov_1saqpwvg6u().f[9]++;
      cov_1saqpwvg6u().s[39]++;
      if (animationFrameRef.current) {
        /* istanbul ignore next */
        cov_1saqpwvg6u().b[15][0]++;
        cov_1saqpwvg6u().s[40]++;
        cancelAnimationFrame(animationFrameRef.current);
        /* istanbul ignore next */
        cov_1saqpwvg6u().s[41]++;
        animationFrameRef.current = null;
      } else
      /* istanbul ignore next */
      {
        cov_1saqpwvg6u().b[15][1]++;
      }
    }
  };
};
